services:
  apiback:
    build:
      context: G:\f16kube\kubenet
      dockerfile: apiback/Dockerfile
    image: apiback
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5001
      published: 5001
      protocol: tcp
  mvcfront:
    build:
      context: G:\f16kube\kubenet
      dockerfile: mvcfront/Dockerfile
    depends_on:
      sqldata:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: mvcfront
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    - mode: ingress
      target: 5000
      published: 5000
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\Administrator\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
  sqldata:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Pass@word
    image: mcr.microsoft.com/mssql/server:2017-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: 1433
      protocol: tcp
networks:
  default:
    name: dockercompose5428643214091499088_default